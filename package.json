{
	"name": "@ctx-core/pagination",
	"version": "13.0.5",
	"description": "ctx-core pagination",
	"keywords": [
		"ctx-core",
		"pagination"
	],
	"homepage": "https://github.com/ctx-core/pagination#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/pagination/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/pagination.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist && npm run clean_tsbuildinfo",
		"clean_tsbuildinfo": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.d.ts",
		"compile": "npm run compile_source && npm run compile_declaration",
		"compile_source": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"compile_declaration": "npm run clean_tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir dist",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.41",
		"@ctx-core/dom": "workspace:^11.0.48",
		"@ctx-core/fontawesome": "workspace:^16.0.10",
		"@ctx-core/nanostores": "workspace:^0.3.0",
		"@ctx-core/ripple-effect": "workspace:^13.0.7",
		"@ctx-core/ripple-effect-ui-svelte": "workspace:^1.0.10",
		"@ctx-core/sass": "workspace:^12.1.18",
		"@ctx-core/svelte": "workspace:^10.1.1"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.121",
		"rimraf": "^3.0.2",
		"svelte": "^3.44.3",
		"svelte-preprocess": "^4.10.1",
		"svelte2tsx": "^0.4.12",
		"tslib": "^2.3.1",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./dist/index.js"
}
