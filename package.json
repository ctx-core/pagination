{
	"name": "@ctx-core/pagination",
	"version": "12.0.107",
	"description": "ctx-core pagination",
	"keywords": [
		"ctx-core",
		"pagination"
	],
	"homepage": "https://github.com/ctx-core/pagination#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/pagination/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/pagination/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"module": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.32",
		"@ctx-core/dom": "workspace:^11.0.39",
		"@ctx-core/fontawesome": "workspace:^15.1.13",
		"@ctx-core/ripple-effect": "workspace:^12.0.75",
		"@ctx-core/sass": "workspace:^12.1.7",
		"@ctx-core/store": "workspace:^27.0.45",
		"svelte": "3.44.2"
	},
	"devDependencies": {
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.85",
		"@swc/cli": "^0.1.52",
		"@swc/core": "^1.2.118",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
