	"devDependencies": {
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.87",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120",
		"rimraf": "^3.0.2",
