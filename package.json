{
	"name": "@ctx-core/pagination",
	"version": "13.0.35",
	"description": "ctx-core pagination",
	"keywords": [
		"ctx-core",
		"pagination"
	],
	"homepage": "https://github.com/ctx-core/pagination#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/pagination/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/pagination.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./lib/index.d.ts",
	"exports": {
		".": {
			"import": "./lib/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf lib && npm run clean:tsbuildinfo",
		"clean:tsbuildinfo": "rimraf tsconfig.tsbuildinfo && rimraf lib/**/*.d.ts",
		"compile": "npm run compile:source && npm run compile:declaration",
		"compile:source": "swc src --out-dir lib --copy-files --source-maps --config-file .swcrc",
		"compile:declaration": "npm run clean:tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir lib",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "*",
		"@ctx-core/dom": "^11.1.8",
		"@ctx-core/fontawesome": "^16.0.20",
		"@ctx-core/nanostores": "^1.0.7",
		"@ctx-core/ripple-effect": "^13.1.5",
		"@ctx-core/ripple-effect-ui-svelte": "^1.0.17",
		"@ctx-core/sass": "^12.1.28",
		"@ctx-core/svelte": "^10.1.8"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.156",
		"rimraf": "^3.0.2",
		"svelte": "^3.46.4",
		"svelte-preprocess": "^4.10.4",
		"svelte2tsx": "^0.5.6",
		"tslib": "^2.3.1",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./lib/index.js",
	"sideEffects": false
}